  <div class="col-md-2 p-2">
    <%= image_tag post.image, class: 'img-fluid', style: 'width:200px;height:auto;border-radius:10px;'%>
  </div>
  <div class="col-md-2 p-2">
    <h5 class="card-title"><%= post.title %></h5>
     <%= link_to "Go to Post", post , class:'btn btn-primary p-1 m-2' %>
     <%= button_to "Delete", post, method: :delete, form: { data: { turbo_confirm: 'Are you sure?' } }, class: 'btn btn-danger p-1 m-2'%>
  </div>

